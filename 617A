import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int c=0,n=0;
        int m;
        m=sc.nextInt();
        if(m<=5)
        {
            System.out.println("1");
        }
        else if(m%5==0)
        {
            System.out.println(m/5);
        }
        else System.out.print((m/5)+1);
    }
}
